{
  "ConnectionStrings": {
    "DefaultConnection": "Host=localhost;Port=5433;Database=market-engine-db;Username=postgres;Password=admin;SslMode=Prefer;Timeout=10"
  },
  //"Logging": {
  //  "LogLevel": {
  //    "Default": "Information",
  //    "Microsoft.AspNetCore": "Warning"
  //  }
  //},
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.Seq" ],
    "MinimumLevel": "Information",
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "Seq",
        "Args": { "serverUrl": "http://localhost:5341" }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ],
    "Properties": {
      "Application": "MarketEngineAPI"
    }
  },
  "AllowedHosts": "*"
}
